<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="productionPoint" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>Gold Washplant Mining</name>
        <image>store.png</image>
        <price>96000</price>
		<canBeSold>true</canBeSold>
        <showInStore>true</showInStore>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>lizard</brand>
        <species>placeable</species>
        <category>productionPoints</category>
		<brush>
            <type>placeable</type>
            <category>production</category>
            <tab>factories</tab>
        </brush>
        <vertexBufferMemoryUsage>50000</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>10000</indexBufferMemoryUsage>
        <textureMemoryUsage>500000</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <filename>washplant.i3d</filename>
    </base>
	
	<hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="unloadTriggerMarker" teleportNode="teleportNode" />
    </hotspots>

    <placement useRandomYRotation="false" useManualYRotation="true" />

    <infoTrigger triggerNode="infoTrigger" />
	
	<clearAreas>
        <clearArea startNode="0>0|0" widthNode="0>0|0|0" heightNode="0>0|0|1"/>
		<clearArea startNode="0>0|1" widthNode="0>0|1|0" heightNode="0>0|1|1"/>
    </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="10" maxSlope="90" maxEdgeAngle="90" >
        <levelAreas>
            <levelArea startNode="0>1|0" widthNode="0>1|0|0" heightNode="0>1|0|1" groundType="dirt"/>
			<levelArea startNode="0>1|1" widthNode="0>1|1|0" heightNode="0>1|1|1" groundType="dirt"/>
			<levelArea startNode="0>1|2" widthNode="0>1|2|0" heightNode="0>1|2|1" groundType="dirt"/>
		</levelAreas>
    </leveling>

    <indoorAreas>
        <indoorArea startNode="0>2|0" widthNode="0>2|0|0" heightNode="0>2|0|1" />
    </indoorAreas>
	
	<tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="0>4|0" endNode="0>4|0|0" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas />
    </ai>

    <triggerMarkers>
        <triggerMarker node="warningStripes_palletSpawnArea" adjustToGround="true" />
        <triggerMarker node="playerTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="WRENCH" adjustToGround="true" />
        <triggerMarker node="unloadTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="false" />
		<triggerMarker node="unloadTriggerMarker2" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
		<triggerMarker node="unloadTriggerMarker3" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
    </triggerMarkers>

    <productionPoint>
        <productions sharedThroughputCapacity="false">
			<production id="STONE" name="Pedra" params="l10n_fillType_concentrate | l10n_fillType_stone | l10n_fillType_dirt |l10n_fillType_tailings" cyclesPerHour="5" costsPerActiveHour="1000">
                <inputs>
                    <input fillType="STONE" amount="17000" />
					<input fillType="DIESEL" amount="300" />
                </inputs>
                <outputs>
					<output fillType="TAILINGS" amount="16450" />
                    <output fillType="CONCENTRATE"  amount="100" />
                    <output fillType="GEMDIRT"  amount="450" />
                </outputs>
            </production>

            <production id="DIRT" name="Terra"  cyclesPerHour="5" costsPerActiveHour="1000">
                <inputs>
                    <input fillType="DIRT" amount="14000" />
					<input fillType="DIESEL" amount="300" />
                </inputs>
                <outputs>
					<output fillType="TAILINGS" amount="13450" />
                    <output fillType="CONCENTRATE"  amount="100" />
                    <output fillType="GEMDIRT"  amount="450" />
                </outputs>
            </production>
		
            <production id="CONCASSE" name="Pedra Azura" params="l10n_fillType_concentrate | l10n_fillType_stone | l10n_fillType_gemdirt |l10n_fillType_tailings" cyclesPerHour="5" costsPerActiveHour="1000">
                <inputs>
                    <input fillType="CONCASSE" amount="17000"/>
					<input fillType="DIESEL" amount="300"/>
                </inputs>
                <outputs>
					<output fillType="TAILINGS" amount="16450" />
                    <output fillType="CONCENTRATE"  amount="100" />
                    <output fillType="GEMDIRT"  amount="450" />
                </outputs>
            </production>

            <production id="GemDirt" name="Terra Azura"  cyclesPerHour="5" costsPerActiveHour="1000">
                <inputs>
                    <input fillType="DIRT1" amount="14000"/>
					<input fillType="DIESEL" amount="300"/>
                </inputs>
                <outputs>
					<output fillType="TAILINGS" amount="13450" />
                    <output fillType="CONCENTRATE"  amount="100" />
                    <output fillType="GEMDIRT"  amount="450" />
                </outputs>
            </production>

        </productions>

        <storage isExtension="false" fillLevelSyncThreshold="100" >
            <capacity fillType="CONCASSE"   capacity="5000" />
            <capacity fillType="DIRT1"   capacity="500000" />
			<capacity fillType="STONE"   capacity="500000" />
            <capacity fillType="DIRT"   capacity="500000" />

			<capacity fillType="DIESEL"   capacity="5000" />
			<capacity fillType="TAILINGS"   capacity="500000" />
            <fillPlane fillType="TAILINGS" node="tailingsHill" minY="-3.25" maxY="0" />
            <capacity fillType="CONCENTRATE"   capacity="5000" />
            <capacity fillType="GEMDIRT"   capacity="500000" />
        </storage>

        <sellingStation node="CONCASSE" supportsExtension="false">
            <unloadTrigger exactFillRootNode="unloadTrigger" fillTypes="CONCASSE DIRT1 DIRT STONE" priceScale=".5" aiNode="aiNode" />
			<unloadTrigger exactFillRootNode="unloadTrigger2" fillTypes="DIESEL" priceScale=".5"  aiNode="aiNode2" />
        </sellingStation>
		
		<loadingStation node="loadingStation" supportsExtension="false">
            <loadTrigger fillTypes="TAILINGS" triggerNode="loadTrigger" fillLitersPerSecond="1000" dischargeNode="dischargeNode" aiNode="loadingStationAINode">
            </loadTrigger>
        </loadingStation>
		
        <palletSpawner>
            <spawnPlaces>
                <spawnPlace startNode="spawnSpace1start" />
                <spawnPlace startNode="spawnSpace2start" />
            </spawnPlaces>
        </palletSpawner>

        <playerTrigger node="playerTrigger" />
		
