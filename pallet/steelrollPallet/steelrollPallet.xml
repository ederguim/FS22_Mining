<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>Steel Pallet</name>
        <image>textures/steelrollPallet_store.png</image>
        <category>misc</category>
		<price>700</price>

        <brand>lizard</brand>
        <showInStore>false</showInStore>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>75264</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>14848</indexBufferMemoryUsage>
        <textureMemoryUsage>2031616</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>pallet/steelrollPallet/steelrollPallet.i3d</filename>
        <size width="1.4" length="1" height="1.1" />
        <canBeReset>false</canBeReset>
        <components>
            <component centerOfMass="0 0.1 0" solverIterationCount="10" mass="250"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
    </base>
  



    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="STEEL" capacity="1000">
                        <fillLevelAnimation name="fillAnimation" resetOnEmpty="true" />
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="200" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <dynamicMount triggerNode="dynamicMountTrigger" />


    <i3dMappings>
        <i3dMapping id="bakeryBoxPallet_component1" node="0>" />
        <i3dMapping id="euroPalletVis" node="0>0" />
        <i3dMapping id="pallet02_mat" node="0>0|0" />
        <i3dMapping id="dynamicMountTrigger" node="0>1" />
        <i3dMapping id="dischargeNode" node="0>2" />
	
        <i3dMapping id="strap" node="0>4|11" />		
		
		
		

        <i3dMapping id="bakeryBoxCol01" node="0>5|0" />
        <i3dMapping id="bakeryBoxCol02" node="0>5|1" />
        <i3dMapping id="bakeryBoxCol03" node="0>5|2" />
        <i3dMapping id="bakeryBoxCol04" node="0>5|3" />

    </i3dMappings>
</vehicle>
