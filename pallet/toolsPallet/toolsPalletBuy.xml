<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>Tools Pallet</name>
        <image>fillPlanesTP/fruit_tp_hud/hud_fill_tools.dds</image>
												<functions>
            <function>Produced by fab shop placeable</function>
        </functions>
        <brand>LAMBO</brand>
        <category>pallets</category>
        <showInStore>true</showInStore>
		<price>1700</price>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>58624</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>11520</indexBufferMemoryUsage>
        <textureMemoryUsage>2031616</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>pallet/plasticsPallet/plasticsPallet.i3d</filename>
        <size width="1.4" length="1" height="0.8" />
        <canBeReset>false</canBeReset>
        <components>
            <component centerOfMass="0 0.1 0" solverIterationCount="10" mass="50"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
    </base>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="TOOLS" capacity="500" startFillType="TOOLS" startFillLevel="500" ignoreFillLimit="true" 
>
                        <fillLevelAnimation name="animation" resetOnEmpty="true" />
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="150" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="pallet_vis">
        <content>
            <object node="furnitureStack01"/>
            <object node="furnitureStack02"/>
            <object node="furnitureStack03"/>
            <object node="furnitureStack04"/>
            <object node="furnitureStack05"/>
            <object node="furnitureStack06"/>
            <object node="furnitureStack07"/>
            <object node="furnitureStack08"/>
            <object node="furnitureStack09"/>
        </content>

        <sounds>
            <unload template="palletsFillPaper" pitchScale="0.9" />
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="0.7" jointTransY="0.11" jointLimitToRotY="true"/>
   <dynamicallyLoadedParts>
        <dynamicallyLoadedPart filename="pallet/barrelPallet/labels.i3d" node="5" linkNode="0>0|2|8|0" />
    </dynamicallyLoadedParts>
    <i3dMappings>
        <i3dMapping id="furnitureStackPallet_component1" node="0>" />
        <i3dMapping id="pallet_vis" node="0>0" />
        <i3dMapping id="dynamicMountTrigger" node="0>0|0" />
        <i3dMapping id="dischargeNode" node="0>0|1" />
        <i3dMapping id="furnitureStack01" node="0>0|2|0" />
        <i3dMapping id="furnitureStack02" node="0>0|2|1" />
        <i3dMapping id="furnitureStack03" node="0>0|2|2" />
        <i3dMapping id="furnitureStack04" node="0>0|2|3" />
        <i3dMapping id="furnitureStack05" node="0>0|2|4" />
        <i3dMapping id="furnitureStack06" node="0>0|2|5" />
        <i3dMapping id="furnitureStack07" node="0>0|2|6" />
        <i3dMapping id="furnitureStack08" node="0>0|2|7" />
        <i3dMapping id="furnitureStack09" node="0>0|2|8" />
    </i3dMappings>
</vehicle>
